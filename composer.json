{
	"name": "aik099/qa-tools",
	"description": "Library for functional testing",
	"keywords": ["PageObject", "HtmlElements", "BEM", "Mink", "Tests"],
	"license": "BSD-3-Clause",

	"authors": [
		{
			"name":  "Alexander Obuhovich",
			"email": "aik.bold@gmail.com"
		},
		{
			"name":  "Dmitry Kushnikov",
			"email": "dkushnikov@gmail.com"
		}
	],

	"require": {
		"php": ">=5.3.1",
		"behat/mink": "~1.5@dev",
		"mindplay/annotations": "dev-annotation-context-feat"
	},

	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"brianium/paratest": "dev-master",
		"mockery/mockery": "dev-master"
	},

	"suggest": {
		"aik099/phpunit-mink": "Allows to use PageObject inside PHPUnit tests and remotely collect code coverage information",
		"phpunit/phpunit": "Allows to use PageObject inside PHPUnit tests"
	},

	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/aik099/php-annotations"
		}
	],

	"autoload": {
		"psr-0": {
			"aik099\\QATools\\": "./library/",
			"tests\\aik099\\QATools\\": "./"
		}
	},

	"extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
